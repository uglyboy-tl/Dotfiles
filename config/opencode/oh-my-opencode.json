{
	"$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
	"google_auth": false,
	"agents": {
		"sisyphus": {
			"model": "opencode/glm-5-free"
		},
		"oracle": {
			"model": "n1n/gpt-5.2"
		},
		"hephaestus": {
			"model": "n1n/gpt-5-codex"
		},
		"librarian": {
			"model": "opencode/minimax-m2.5-free"
		},
		"explore": {
			"model": "opencode/minimax-m2.5-free"
		},
		"frontend-ui-ux-engineer": {
			"model": "google/antigravity-gemini-3-pro"
		},
		"document-writer": {
			"model": "google/antigravity-gemini-3-flash"
		},
		"multimodal-looker": {
			"model": "google/antigravity-gemini-3-flash"
		},
		"prometheus": {
			"model": "n1n/gpt-5.2"
		},
		"momus": {
			"model": "n1n/gpt-5.2"
		},
		"atlas": {
			"model": "google/antigravity-claude-sonnet-4-6"
		}
	},
	"categories": {
		"ultrabrain": {
			"model": "n1n/gpt-5-codex"
		},
		"deep": {
			"model": "n1n/gpt-5-codex"
		},
		"quick": {
			"model": "deepseek/deepseek-chat"
		},
		"writing": {
			"model": "n1n/gemini-3-flash"
		}
	},
	"tmux": {
		"enabled": true,
		"layout": "main-vertical"
	},
	"disabled_hooks": [
		"todo-continuation-enforcer",
		"anthropic-context-window-limit-recovery",
		"auto-update-checker",
		"claude-code-hooks"
	],
	"disable_agents": [],
	"disabled_skills": [
		"playwright"
	],
	"disabled_commands": [
		"init-deep"
	],
	"claude_code": {
		"mcp": false,
		"commands": false,
		"skills": false,
		"agents": false,
		"hooks": false,
		"plugins": false
	}
}